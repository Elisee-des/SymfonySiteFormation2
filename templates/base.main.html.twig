	<body>

		<!-- ***** Preloader Start ***** -->
		<div id="js-preloader" class="js-preloader">
			<div class="preloader-inner">
				<span class="dot"></span>
				<div class="dots">
					<span></span>
					<span></span>
					<span></span>
				</div>
			</div>
		</div>
		<!-- ***** Preloader End ***** -->

		<!-- ***** Header Area Start ***** -->
		{% include "main/includes/header.html.twig" %}
		<br><br><br><br>

		{% block main %}

			<!-- ***** Header Area End ***** -->
			{% include "main/main.html.twig" %}

			<div id="services" class="our-services section">
				<div class="services-right-dec">
					<img src="{{ asset("asset/images/services-right-dec.png") }}" alt="">
				</div>
				<div class="container">
					<div class="services-left-dec">
						<img src="{{ asset("asset/images/services-left-dec.png") }}" alt="">
					</div>
					<div class="row">
						<div class="col-lg-6 offset-lg-3">
							<div class="section-heading">
								<h2>Nos
									<em>Categories</em>
									de
									<span>Formation</span>
								</h2>
								<span>Nos Categories</span>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="col-lg-12">
							<div class="owl-carousel owl-services">
								{% block categories %}{% endblock %}
							</div>
						</div>
					</div>
				</div>
			</div>

			{% include "main/apropos.html.twig" %}

			{% include "main/portfolio.html.twig" %}

			{% include "main/contact.html.twig" %}

			<div class="footer-dec">
				<img src="{{ asset("asset/images/footer-dec.png") }}" alt="">
			</div>

		{% endblock %}

		<br>
		<br>
		<br>
		<br><br><br>
		{% include "main/includes/footer.html.twig" %}

		<!-- Scripts -->
		<script src="{{ asset("vendor/jquery/jquery.min.js") }}"></script>
		<script src="{{ asset("vendor/bootstrap/js/bootstrap.bundle.min.js") }}"></script>
		<script src="{{ asset("asset/js/owl-carousel.js") }}"></script>
		<script src="{{ asset("asset/js/animation.js") }}"></script>
		<script src="{{ asset("asset/js/imagesloaded.js") }}"></script>
		<script src="{{ asset("asset/js/custom.js") }}"></script>

		<script>
			// Acc
$(document).on("click", ".naccs .menu div", function () {
var numberIndex = $(this).index();

if (!$(this).is("active")) {
$(".naccs .menu div").removeClass("active");
$(".naccs ul li").removeClass("active");

$(this).addClass("active");
$(".naccs ul").find("li:eq(" + numberIndex + ")").addClass("active");

var listItemHeight = $(".naccs ul").find("li:eq(" + numberIndex + ")").innerHeight();
$(".naccs ul").height(listItemHeight + "px");
}
});
		</script>
	</body>
</html>
